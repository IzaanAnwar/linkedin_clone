{
    "extends": ["prettier", "next"],
    "plugins": ["prettier", "react-hooks"],

    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off",
        "react/jsx-key": "off",
        "react-hooks/rules-of-hooks": "off"
    },
    "parserOptions": { "sourceType": "module" }
}
